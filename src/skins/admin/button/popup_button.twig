{##
 # Popup button
 #}
<button type="button" class="{{ this.getClass() }}">
  {{ this.displayCommentedData(this.getURLParams()) }}
{% if this.getParam('icon-style') %}
  <i class="button-icon {{ this.getParam('icon-style') }}"></i>
{% endif %}
  <span>{{ t(this.getButtonContent()) }}</span>
</button>
